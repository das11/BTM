    <script>
        $(document).ready(function() {

            $("#tab").tabpager({

                //  maximum visible items
                items: 1,

                // CSS class for tabbed content
                contents: 'contents',

                // transition speed
                time: 100,

                // text for previous button
                previous: ' &nbsp;',

                // text for next button
                next: '&nbsp;',

                // initial tab
                start: 1,

                // top or bottom
                position: 'top',

                // scrollable
                scroll: true
            });

            // No Slider 
            $('#jquery-tab-pager-navi').hide();
        });
    </script>
    <script>
        $(function() {
            var items = $('.tabss>#tab>li').each(function() {
                $(this).click(function() {
                    //remove previous class and add it to clicked tab
                    items.removeClass('ds_non');
                    $(this).addClass('ds_non');

                    //hide all content divs and show current one
                    $('.tabss .director_text').hide().eq(items.index($(this))).show();

                    $('.tabss .director_text').hide().eq(items.index($(this))).fadeIn(1000);

                    $('.tabss .director_text').hide().eq(items.index($(this))).show();
                    //window.location.hash = $(this).attr('view');
                });
            });

        });
    </script>
    <script>
        $(function() {
            var items = $('.tabss>#tab>#arrows').each(function() {
                $(this).click(function() {
                    //remove previous class and add it to clicked tab
                    //items.removeClass('ds_non');
                    //$(this).addClass('ds_non');

                    //hide all content divs and show current one
                    //$('.tabss #jquery-tab-pager-navi').show().eq(items.index($(this))).hide();

                    //$('.tabss #jquery-tab-pager-navi').show().eq(items.index($(this))).fadeIn(1000);    

                    $('.tabss #jquery-tab-pager-navi').show().eq(items.index($(this))).hide();
                    //window.location.hash = $(this).attr('view');
                });
            });

        });
    </script>

    <script src="js/TweenMax.min.js" type="text/javascript"></script>
    <script>
        //replace this js with yours.

        //the following is just an example

        $(".dir_tor_box ").hover(over, out);

        function over() {
            //TweenMax.to(this, 0.5, {backgroundColor:"red"})
            //TweenMax.to($(this).find("img"), 0.2, {rotation:360, scale:2, x:50, delay:0.6})
            TweenMax.to($(this).find(".ovel"), 0.3, {
                top: 0,
                delay: 0.1
            })
            TweenMax.to($(this).find(".ic_sh_fb"), 0.4, {
                right: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find(".ic_sh_tw"), 0.5, {
                right: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find(".ic_sh_in"), 0.6, {
                right: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find("#di_name"), 0.8, {
                opacity: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find("#di_pos"), 0.9, {
                opacity: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find("#di_dis"), 1, {
                opacity: 1,
                delay: 0.3
            })
            TweenMax.to($(this).find("#di_more"), 0.4, {
                opacity: 1,
                delay: 0.2
            })

        }

        function out() {
            //TweenMax.to(this, 0.5, {backgroundColor:"blue"})
            TweenMax.to($(this).find(".ovel"), 0.3, {
                top: 1500,
                overwrite: "all"
            })
            TweenMax.to($(this).find(".ic_sh_fb"), 0.1, {
                right: -130,
                overwrite: "all"
            })
            TweenMax.to($(this).find(".ic_sh_tw"), 0.1, {
                right: -130,
                overwrite: "all"
            })
            TweenMax.to($(this).find(".ic_sh_in"), 0.1, {
                right: -130,
                overwrite: "all"
            })
            TweenMax.to($(this).find("#di_name"), 0, {
                opacity: 0,
                overwrite: "all"
            })
            TweenMax.to($(this).find("#di_pos"), 0, {
                opacity: 0,
                overwrite: "all"
            })
            TweenMax.to($(this).find("#di_dis"), 0, {
                opacity: 0,
                overwrite: "all"
            })
            TweenMax.to($(this).find(".cl_ent_overlay"), 0, {
                opacity: 0,
                overwrite: "all"
            })
            TweenMax.to($(this).find("#di_more"), 0, {
                opacity: 0,
                overwrite: "all"
            })
        }
    </script>
    <script>
        //replace this js with yours.

        //the following is just an example

        $(".cl_ent_box").hover(over, out);

        function over() {

            TweenMax.to($(this).find(".cl_ent_overlay"), 0.9, {
                opacity: 0,
                delay: 0.3
            })
        }

        function out() {

            TweenMax.to($(this).find(".cl_ent_overlay"), 0.9, {
                opacity: 1,
                overwrite: "all"
            })
        }
    </script>
    <script>
        //replace this js with yours.

