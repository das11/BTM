    <script>

        $("#review").click(function(event) {
            $("#press").removeClass('active');
            $("#industry").removeClass('active');
            $(this).addClass('active');
            $(".review").fadeIn();
            $(".press").fadeOut();
            $(".industry").fadeOut();
        });

        $("#press").click(function(event) {
            $("#review").removeClass('active');
            $("#industry").removeClass('active');
            $(this).addClass('active');
            $(".press").fadeIn();
            $(".review").fadeOut();
            $(".industry").fadeOut();
        });

        $("#industry").click(function(event) {
            $("#press").removeClass('active');
            $("#review").removeClass('active');
            $(this).addClass('active');
            $(".industry").fadeIn();
            $(".review").fadeOut();
            $(".press").fadeOut();
        });

        $(".n-contact").click(function(event) {
            $(".navbar-nav li").removeClass('active')
            $(this).parent().addClass('active')

        });

        $(".n-client").click(function(event) {
            $(".navbar-nav li").removeClass('active');
            $(this).parent().addClass('active')
        });

        $(".n-media").click(function(event) {
            $(".navbar-nav li").removeClass('active');
            $(this).parent().addClass('active')
        });

        

    </script>        
