        $(function() {

            var filterList = {

                init: function() {

                    // MixItUp plugin
                    // http://mixitup.io
                    $('#portfoliolist').mixItUp({
                        selectors: {
                            target: '.wor_k_box , .wor_k_box_right, .wor_k_box_bottom',
                            filter: '.filter'
                        },
                        load: {
                            filter: '.app'
                        }
                    });

                }

            };

            // Run the show!
            filterList.init();

        });
    </script>

